{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "preview"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "production"
      }
    },
    "local": {
      "android": {
        "buildType": "apk",
        "distribution": "internal",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentials": true
      },
      "env": {
        "APP_ENV": "preview"
      }
    },
    "github": {
      "android": {
        "buildType": "apk",
        "distribution": "internal",
        "autoIncrement": true
      },
      "env": {
        "APP_ENV": "preview"
      },
      "cache": {
        "key": "github-{{ hashFiles('package-lock.json') }}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}